capture_directory: "~"
fixed_frame: map
target_frame: map
fix_orientation: false
rotate_90: false
enable_antialiasing: true
show_displays: true
show_status_bar: true
show_capture_tools: true
window_width: 1208
window_height: 736
view_scale: 0.866783917
offset_x: 71.2234268
offset_y: -34.0852661
use_latest_transforms: true
background: "#a0a0a4"
image_transport: raw
displays:
